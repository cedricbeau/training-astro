---
import Accordion from '@components/Accordion.astro';
import Avatar from '@components/Avatar.astro';
import Card from '@components/Card.astro';
import ComponentContainer from '@components/ComponentContainer.astro';
import Layout from '@layouts/Layout.astro';
import Slideshow from '@components/Slideshow.astro';
import Tabz from '@components/Tabz.astro';
import Timeline from '@components/Timeline.astro';
---

<Layout title="Data display">
        <ComponentContainer id="accordion" title="Accordion">
            <Accordion />
        </ComponentContainer>
        <ComponentContainer id="avatar" title="Avatar">
            <Avatar />
        </ComponentContainer>
        <ComponentContainer id="card" title="Card">
            <Card />
        </ComponentContainer>
        <ComponentContainer id="slideshow" title="Slideshow">
            <Slideshow />
        </ComponentContainer>

        <ComponentContainer id="tabz" title="Tabz">
            <Tabz />
        </ComponentContainer>
        <ComponentContainer id="timeline" title="Timeline">
            <Timeline />
        </ComponentContainer>
</Layout>
